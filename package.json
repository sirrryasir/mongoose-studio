{
  "name": "mongoose-studio",
  "version": "1.1.0",
  "description": "A modern, zero-config GUI for your Mongoose models. Run instantly with npx mongoose-studio.",
  "keywords": [
    "mongoose",
    "gui",
    "studio",
    "admin",
    "mongodb",
    "cli",
    "database",
    "dashboard",
    "visualizer",
    "developer-tools",
    "introspection"
  ],
  "author": "Yasir",
  "license": "MIT",
  "homepage": "https://mongoose-studio.yaasir.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/sirrryasir/mongoose-studio"
  },
  "bugs": {
    "url": "https://github.com/sirrryasir/mongoose-studio/issues"
  },
  "module": "index.ts",
  "type": "module",
  "bin": {
    "mongoose-studio": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx bin/index.ts",
    "clean": "rimraf dist ui/out",
    "build": "npm run clean && (cd ui && npm run build) && mkdir -p dist/ui && cp -r ui/out/* dist/ui/ && tsup bin/index.ts --format esm --out-dir dist",
    "prepublishOnly": "bun run build",
    "test": "bun test"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@types/bun": "latest",
    "mongoose": "^9.1.5",
    "tsup": "^8.5.1",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^25.2.1",
    "commander": "^14.0.3",
    "glob": "^13.0.1",
    "hono": "^4.11.7",
    "mime-types": "^3.0.2",
    "open": "^11.0.0",
    "tsx": "^4.21.0"
  }
}